{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "no-extra-semi": "off",
      "class-methods-use-this": "off",
      "no-param-reassign": "off"
    }
}
